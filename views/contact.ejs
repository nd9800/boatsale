<section class="headcontact" style="    background: url(img/sea-2.jpeg) ;"></section>
<section class="contactth">
    <div class="contentth">
        <h2>How can we help you?</h2>
        <p>Feel free to reach out to us using the contact information below.
        </p>
    </div>
    <div class="containerth">
        <div class="contactinfoth">
            <div class="boxth">
                <div class="iconth"><i class="fa fa-map-marker" aria-hidden="true"></i> </div>
                <div class="textth">
                    <h3>Address</h3>
                    <p><b style="font-size: 24px">Florida</b>,USA</p>
                </div>
            </div>
            <div class="boxth">
                <div class="iconth"><i class="fa fa-phone" aria-hidden="true"></i></div>
                <div class="textth">
                    <h3>Phone</h3>
                    <p><a href="tel:+1 (954) 715-4114" style="text-decoration: none; color:black">+1 (954) 715-4114</a></p>
                </div>
            </div>
            <div class="boxth">
                <div class="iconth"><i class="fa fa-envelope-o" aria-hidden="true"></i></div>
                <div class="textth">
                    <h3>Mail</h3>
                    <p><a href="mailto:global@boatsetter.com" style="text-decoration: none; color:black">global@boatsetter.com</a></p>
                </div>
            </div>
        </div>
        <div class="contactformth ladi-element"  id="FORM2" data-config-id="5f5e2847785b8432afddeaa1">
            <form autocomplete="off" method="post" class='ladi-form' >
                <div  id="BUTTON3" class="ladi-element">
                    <div class='ladi-button'><div class="ladi-button-background">

                        </div><div  id="BUTTON_TEXT3" class="ladi-element">
                            <h2 class='ladi-headline'>Send Message</h2>
                        </div>
                    </div>
                </div>
                <div id="FORM_ITEM4" class="ladi-element">
                    <div class="ladi-form-item-container">
                        <div class="ladi-form-item-background">

                        </div><div class='ladi-form-item'>
                            <div class="inputboxth">
                                <input autocomplete="off" tabindex="1" name="name" required class="ladi-form-control" type="text" required="required">
                                <span>Full Name</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="FORM_ITEM5" >
                    <div class="ladi-form-item-container">
                        <div class="ladi-form-item-background">

                        </div>
                        <div class='ladi-form-item'>
                            <div class="inputboxth" >
                                <input autocomplete="off" tabindex="2" name="email" class="ladi-form-control" type="text"  required="required">
                                <span>Mail</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div id="FORM_ITEM7" class="ladi-element">
                    <div class="ladi-form-item-container">
                        <div class="ladi-form-item-background">

                        </div><div class='ladi-form-item'>
                            <div class="inputboxth" >
                                <textarea  autocomplete="off" tabindex="4" name="message" class="ladi-form-control" required="required"></textarea>
                                <span style="">Type Your Message</span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="inputboxth">
                    <input type="submit" class="ladi-hidden" value="Send">
                </div>
            </form>
        </div>
    </div>

</section>
